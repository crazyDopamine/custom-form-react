{
  "extends": ["alloy", "alloy/react", "alloy/typescript"],
  "env": {
    "browser": true
  },
  "globals": {
    "moment": true
  },
  "rules": {
    "complexity": 0,
    "max-params": 0,
    "default-case-last": 0,
    "no-loss-of-precision": 0,
    "no-useless-backreference": 0,
    "no-param-reassign": 0,
    "eqeqeq": 0,
    "default-case": 0,
    "no-self-compare": 0,
    "no-useless-escape": 0,
    "jsx-a11y/anchor-is-valid": 0,
    "jsx-a11y/alt-text": 0,
    "array-callback-return": 0,
    "no-eval": 0,
    "no-useless-constructor": 0,
    "prettier/prettier": 2
  },
  "plugins": ["prettier"]
}
