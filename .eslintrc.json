{
  "parserOptions": {
    "parser": "babel-eslint"
  },
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": ["react-app", "prettier"],
  "globals": {
    "moment": true
  },
  "rules": {
    "eqeqeq": 0,
    "default-case": 0,
    "no-self-compare": 0,
    "no-useless-escape": 0,
    "jsx-a11y/anchor-is-valid": 0,
    "jsx-a11y/alt-text": 0,
    "array-callback-return": 0,
    "no-eval": 0,
    "no-useless-constructor": 0,
    "prettier/prettier": 2
  },
  "plugins": ["prettier"]
}
